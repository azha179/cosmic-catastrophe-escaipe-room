      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#282743" height="376.0" layoutX="96.0" layoutY="70.0" opacity="0.64" stroke="BLACK" strokeType="INSIDE" width="570.0" />
      <ImageView fitHeight="78.0" fitWidth="233.0" layoutX="97.0" layoutY="60.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/SettingText.png" />
